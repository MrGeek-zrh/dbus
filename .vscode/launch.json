{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach to dbus-daemon",
            "type": "cppdbg",
            "request": "attach",
            "program": "/usr/bin/dbus-daemon",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            // "preLaunchTask": "rebuild-dbus",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "logging": {
                "moduleLoad": true,
                "trace": true,
                "engineLogging": true,
                "programOutput": true,
                "exceptions": true
            },
            "miDebuggerPath": "/usr/bin/gdb",
            "miDebuggerArgs": "",
            "stopAtEntry": false
        }
    ]
}